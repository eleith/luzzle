{
  "name": "@luzzle/prisma",
  "version": "0.0.2",
  "author": "eleith",
  "scripts": {
    "build": "prisma generate --schema ./src/schema.prisma",
    "compile": "tsc && mv prisma dist",
    "patch": "patch dist/prisma/client/runtime/index.js runtime-patch.txt",
    "lint": "eslint index.ts",
    "prepublishOnly": "npm run build && npm run compile && npm run patch"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/prisma/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://git.eleith.com/eleith/eleith.git"
  },
  "homepage": "https://git.eleith.com/eleith/eleith.git",
  "devDependencies": {
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "prisma": "^4.3.1"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
