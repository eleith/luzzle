{
  "name": "@luzzle/kysely",
  "version": "0.0.16",
  "author": "eleith",
  "scripts": {
    "lint": "eslint src",
    "build": "tsc -p tsconfig.prod.json",
    "test": "vitest run --coverage --silent",
		"migration:add": "TIMESTAMP=$(date -u +'%Y-%m-%dT%H:%M:%SZ'); cp ./src/migrations/template.txt ./src/migrations/$TIMESTAMP.ts && echo \"created ./src/migrations/$TIMESTAMP.ts\""
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.eleith.com/eleith/eleith.git"
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "homepage": "https://git.eleith.com/eleith/eleith.git",
  "description": "luzzle's db (kysely) connector",
  "devDependencies": {
    "@types/better-sqlite3": "7.6.4",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vitest/coverage-v8": "0.34.4",
    "@vitest/ui": "0.34.4",
    "esbuild": "^0.15.7",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "4.8.4",
    "vite": "^4.1.4",
    "vitest": "0.34.4"
  },
  "dependencies": {
    "ajv": "8.12.0",
    "better-sqlite3": "8.4.0",
    "kysely": "0.25.0"
  }
}
