{
  "name": "@luzzle/cli",
  "version": "0.0.45",
  "author": "eleith",
  "scripts": {
    "lint": "eslint src",
    "compile": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json && chmod +x dist/src/index.js",
    "postbuild": "sed -i \"s/'unbuilt'/$(npm pkg get version --workspaces=false)/\" dist/src/version.js",
    "test": "vitest run --silent --coverage"
  },
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "luzzle": "dist/src/index.js"
  },
  "files": [
    "dist/*",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://git.eleith.com/eleith/eleith.git"
  },
  "homepage": "https://git.eleith.com/eleith/eleith.git",
  "main": "dist/manager.js",
  "description": "manage folders of markdown files for building luzzle sites",
  "devDependencies": {
    "@types/async": "^3.2.15",
    "@types/node": "18.8.3",
    "@types/sharp": "0.31.0",
    "@types/yargs": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vitest/coverage-v8": "0.34.4",
    "@vitest/ui": "0.34.4",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gaxios": "^5.0.2",
    "prettier": "^2.7.1",
    "typescript": "4.8.4",
    "vite": "^4.1.4",
    "vitest": "0.34.4"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "@googleapis/books": "^0.3.1",
    "@luzzle/kysely": "0.0.14",
    "@paralleldrive/cuid2": "2.2.0",
    "@sindresorhus/slugify": "2.2.0",
    "@types/lodash-es": "4.17.7",
    "ajv": "8.12.0",
    "async": "^3.2.4",
    "conf": "11.0.1",
    "deepmerge": "^4.2.2",
    "file-type": "^18.0.0",
    "got": "12.5.1",
    "lodash-es": "4.17.21",
    "mdast-util-to-markdown": "^1.3.0",
    "openai": "3.2.1",
    "pino": "8.6.1",
    "pino-pretty": "9.1.1",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "sharp": "0.31.2",
    "tempy": "^3.0.0",
    "unified": "^10.1.2",
    "unist-util-filter": "^4.0.0",
    "unist-util-visit": "^4.1.1",
    "yaml": "2.1.3",
    "yargs": "17.6.0"
  },
  "type": "module"
}
