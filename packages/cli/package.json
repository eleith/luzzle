{
  "name": "@luzzle/cli",
  "version": "0.0.17",
  "author": "eleith",
  "scripts": {
    "lint": "eslint src",
    "compile": "tsc --noEmit",
    "build": "esbuild src/manage.ts --platform=node --target=node16 --banner:js='#!/usr/bin/env node' --sourcemap --bundle --outfile=dist/manager.js --external:sharp",
    "test": "vitest run --coverage --silent",
    "prepublishOnly": "npm run build && chmod +x dist/manager.js"
  },
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "luzzle": "dist/manager.js"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://git.eleith.com/eleith/eleith.git"
  },
  "homepage": "https://git.eleith.com/eleith/eleith.git",
  "main": "dist/manager.js",
  "description": "manage folders of markdown files for building luzzle sites",
  "devDependencies": {
    "@types/async": "^3.2.15",
    "@types/lodash": "4.14.186",
    "@types/node": "18.8.3",
    "@types/sharp": "0.31.0",
    "@types/yargs": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "0.25.3",
    "c8": "^7.12.0",
    "esbuild": "^0.15.7",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gaxios": "^5.0.2",
    "prettier": "^2.7.1",
    "typescript": "4.8.4",
    "vite": "^3.2.4",
    "vitest": "0.25.3"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "@googleapis/books": "^0.3.1",
    "@luzzle/prisma": "0.0.17",
    "ajv": "^8.11.0",
    "async": "^3.2.4",
    "conf": "10.2.0",
    "deepmerge": "^4.2.2",
    "dotenv": "16.0.3",
    "file-type": "^18.0.0",
    "got": "12.5.1",
    "lodash": "^4.17.21",
    "mdast-util-to-markdown": "^1.3.0",
    "pino": "8.6.1",
    "pino-pretty": "9.1.1",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "sharp": "0.31.2",
    "tempy": "^3.0.0",
    "unified": "^10.1.2",
    "unist-util-filter": "^4.0.0",
    "unist-util-visit": "^4.1.1",
    "yaml": "2.1.3",
    "yargs": "17.6.0"
  }
}
