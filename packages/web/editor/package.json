{
	"name": "@luzzle/web.editor",
	"version": "0.0.15",
	"private": true,
	"scripts": {
		"dev": "dotenvx run -f .env.local -- vite dev",
		"build": "dotenvx run -f .env.local -- vite build",
		"build:lock": "npm i --package-lock-only --workspaces=false",
		"preview": "dotenvx run -f .env.local -- vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"engines": {
		"node": ">=20"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eleith/luzzle.git"
	},
	"devDependencies": {
		"@iconify-json/ph": "1.2.2",
		"@luzzle/core": "0.0.34",
		"@sveltejs/adapter-node": "5.2.12",
		"@sveltejs/enhanced-img": "0.8.1",
		"@sveltejs/kit": "2.43.0",
		"@sveltejs/vite-plugin-svelte": "6.2.0",
		"@types/eslint": "9.6.1",
		"@types/mime-types": "2.1.4",
		"@types/node": "22.12.0",
		"eslint": "9.18.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "3.9.2",
		"feed": "4.2.2",
		"globals": "15.14.0",
		"mime-types": "3.0.1",
		"prettier": "3.4.2",
		"prettier-plugin-svelte": "3.4.0",
		"svelte": "5.39.4",
		"svelte-check": "4.3.2",
		"typescript": "5.7.3",
		"typescript-eslint": "8.19.1",
		"unplugin-icons": "22.0.0",
		"vite": "7.1.7",
		"webdav": "5.7.1"
	},
	"type": "module",
	"dependencies": {
		"@dotenvx/dotenvx": "1.32.1",
		"@resvg/resvg-js": "2.6.2",
		"@sindresorhus/slugify": "2.2.1",
		"better-sqlite3": "11.7.2",
		"kysely": "0.27.5",
		"satori": "0.12.1",
		"sharp": "0.33.5",
		"vite-node": "2.1.8"
	}
}
