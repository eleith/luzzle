{
	"name": "@luzzle/web",
	"version": "0.0.43",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build:lock": "npm i --package-lock-only --workspaces=false && cp package-lock.json web.package-lock.json",
		"build": "vite build",
		"preview": "vite preview",
		"build:luzzle:db": "vite-node tools/luzzle/index.ts",
		"build:luzzle:images": "vite-node tools/luzzle/images.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"engines": {
		"node": ">=20"
	},
	"repository": {
		"type": "git",
		"url": "git+https://git.eleith.com/eleith/eleith.git"
	},
	"devDependencies": {
		"@iconify-json/ph": "1.2.0",
		"@melt-ui/pp": "0.3.2",
		"@melt-ui/svelte": "0.83.0",
		"@sveltejs/adapter-node": "5.2.5",
		"@sveltejs/enhanced-img": "0.3.7",
		"@sveltejs/kit": "2.6.2",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@types/eslint": "9.6.0",
		"dotenv": "16.4.5",
		"eslint": "9.0.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.44.1",
		"feed": "4.2.2",
		"globals": "15.0.0",
		"marked": "14.1.2",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.2.6",
		"svelte": "5.0.0-next.260",
		"svelte-check": "4.0.4",
		"typescript": "5.4.5",
		"typescript-eslint": "8.8.0",
		"unplugin-icons": "0.19.2",
		"vite": "5.4.8"
	},
	"type": "module",
	"dependencies": {
		"@luzzle/core": "0.0.12",
		"@resvg/resvg-js": "2.6.2",
		"better-sqlite3": "8.4.0",
		"kysely": "0.27.3",
		"satori": "0.11.1",
		"sharp": "0.33.5",
		"vite-node": "2.1.2"
	}
}
